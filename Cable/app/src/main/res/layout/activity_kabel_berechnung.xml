<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mediha.cable.KabelBerechnung">

    <Spinner
        android:id="@+id/spinnerVerlege"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="121dp"
        android:entries="@array/verlaege"
        />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Verlegeart:"
        android:textSize="14sp"
        android:textColor="@color/colorPrimary"
        android:visibility="visible"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="104dp" />

    <CheckBox
        android:id="@+id/checkBox230"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="230 Volt"
        android:textColor="@color/colorPrimary"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="178dp" />

    <CheckBox
        android:id="@+id/checkBox400"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="400 Volt"
        android:textColor="@color/colorPrimary"
        tools:layout_editor_absoluteY="178dp"
        tools:layout_editor_absoluteX="220dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="178dp"
        android:layout_height="42dp"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="198dp"
        tools:layout_editor_absoluteY="250dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Laenge der Leitung in Meter:"
        android:textSize="14sp"
        android:textColor="@color/colorPrimary"
        tools:layout_editor_absoluteY="266dp"
        tools:layout_editor_absoluteX="8dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Leistung in Watt:"
        android:textSize="14sp"
        android:textColor="@color/colorPrimary"
        tools:layout_editor_absoluteY="59dp"
        tools:layout_editor_absoluteX="8dp" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="39dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="317dp" />


</android.support.constraint.ConstraintLayout>
